{
  "name": "@phillui/ts-toolbox",
  "version": "1.0.0",
  "description": "A comprehensive TypeScript functional programming library featuring monads, monad transformers, optics, and utility functions",
  "type": "module",
  "main": "./dist/ts-toolbox.umd.js",
  "module": "./dist/ts-toolbox.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/ts-toolbox.es.js",
      "require": "./dist/ts-toolbox.umd.js"
    }
  },
  "scripts": {
    "build": "vite build && tsc --emitDeclarationOnly",
    "test": "vitest --run"
  },
  "keywords": [
    "typescript",
    "functional-programming",
    "fp"
  ],
  "author": "Phil Lui",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/phillui-37/ts-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/phillui-37/ts-toolbox/issues"
  },
  "homepage": "https://github.com/phillui-37/ts-toolbox",
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "ts-pattern": "^5.9.0"
  },
  "devDependencies": {
    "typescript": "^5.9.3",
    "vite": "^7.2.4",
    "@types/node": "^20.9.0",
    "vitest": "^1.3.0"
  }
}